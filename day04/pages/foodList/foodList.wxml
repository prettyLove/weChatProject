
<view class='foodlist' wx:for="{{list}}" wx:key="index">
<!-- 美食图片 -->
   <image src='http://127.0.0.1:3000/{{item.img_url}}' class='shop-img'></image>
   <!-- 店铺信息 -->
   <view class='shop-info'>
       <view class='shop-dname ell' >店铺名称:{{item.dname}}</view>
       <view class='shop-tel'>电话:{{item.tel}}</view>
       <view class='shop-addr'>地址:{{item.addr}}</view>
       <view class='shop-ctime'>营业时间:{{item.ctime}}</view>
   </view>
   <!-- 打分 -->
   <view> 
      <view class='star-after'>★★★★★</view>
      <view class='star-before' style='width:{{item.point}}%'>★★★★★</view>
   </view>
</view>
